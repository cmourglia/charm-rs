fn fib_rec(n) {
    if n == 0 { return 0; }
    if n == 1 { return 1; }

    return fib_rec(n - 1) + fib_rec(n - 2);
}

fn fib_iter(n) {
    var a = 0;
    var b = 1;

    for var i = 0; i < n; i += 1 {
        var tmp = a + b;
        a = b;
        b = tmp;
    }
    
    return a;
}

var s0 = time();
print(fib_iter(100));
var e0 = time();

print((e0 - s0) / 1000000000);

var s1 = time();
print(fib_rec(25));
var e1 = time();

print((e1 - s1) / 1000000000);

